{
  "name": "svn-helper",
  "version": "1.1.2",
  "author": "Tobias Friemel @tfriem",
  "description": "svn-helper is a command line tool that aims to improve working with multiple subversion repositories at specific versions. For this it provides commands to switch multiple working copies at once.",
  "bin": {
    "svn-helper": "./bin/run"
  },
  "bugs": "https://github.com/tfriem/svn-helper/issues",
  "dependencies": {
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/plugin-help": "^3.1.0",
    "ansi-colors": "^4.1.1",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "fuzzy": "^0.1.3",
    "indent-string": "^4.0.0",
    "inquirer": "^7.3.1",
    "inquirer-autocomplete-prompt": "^1.0.2",
    "listr": "^0.14.3",
    "lodash": "^4.17.19",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/tslint": "^3.1.1",
    "@types/execa": "^2.0.0",
    "@types/fs-extra": "^9.0.1",
    "@types/indent-string": "^4.0.1",
    "@types/inquirer": "6.5.0",
    "@types/jest": "^26.0.4",
    "@types/listr": "^0.14.2",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.22",
    "commitizen": "^4.1.2",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "globby": "^11.0.1",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-junit": "^11.0.1",
    "replace": "^1.2.0",
    "semantic-release": "^17.1.1",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/tfriem/svn-helper",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "svn-helper",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "tfriem/svn-helper",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest --coverage --silent --ci --runInBand",
    "version": "oclif-dev readme && git add README.md",
    "postshrinkwrap": "replace --silent 'http://' 'https://' ./package-lock.json",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "types": "lib/index.d.ts",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
